[coins jingling]

4
00:00:24,482 --> 00:00:25,400
[whooshes]

5
00:00:26,526 --> 00:00:28,820
-[whirring]
-[chiming]

6
00:00:37,412 --> 00:00:40,290
[whooshing]

7
00:00:44,461 --> 00:00:45,378
[chimes]

8
00:00:49,841 --> 00:00:52,969
[coins jingling]

9
00:00:57,474 --> 00:00:58,516
[chimes]

10
